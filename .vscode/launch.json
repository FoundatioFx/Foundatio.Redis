{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Sample Job",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build sample job",
      "program": "${workspaceFolder}/samples/Foundatio.SampleJob/bin/Debug/netcoreapp3.1/Foundatio.SampleJob.dll",
      "args": [],
      "cwd": "${workspaceFolder}/samples/Foundatio.SampleJob",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": "Sample Client",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build sample client",
      "program": "${workspaceFolder}/samples/Foundatio.SampleJobClient/bin/Debug/netcoreapp3.1/Foundatio.SampleJobClient.dll",
      "args": [],
      "cwd": "${workspaceFolder}/samples/Foundatio.SampleJobClient",
      "console": "integratedTerminal",
      "stopAtEntry": false
    },
    {
      "name": "Benchmarks",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/test/Foundatio.Benchmarks/bin/Debug/netcoreapp3.1/Foundatio.Benchmarks.dll",
      "args": [],
      "cwd": "${workspaceFolder}/test/Foundatio.Benchmarks",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ],
  "compounds": [
    {
      "name": "Sample",
      "configurations": [
        "Sample Job",
        "Sample Client"
      ]
    }
  ]
}